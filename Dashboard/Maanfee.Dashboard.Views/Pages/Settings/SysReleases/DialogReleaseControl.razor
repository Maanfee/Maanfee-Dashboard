@code {

    [CascadingParameter]
    public IMudDialogInstance MudDialog { get; set; }

    private async void Close()
    {
        SharedLayoutSettings.CurrentVersion = SysRelease.Version;
        await LocalConfiguration.SetConfigurationAsync();

        MudDialog.Close();
    }

    // *****************************************************

    [Parameter]
    public SysRelease SysRelease { get; set; }

    protected override async Task OnInitializedAsync()
    {
        IsLoaded = false;

        await Task.Delay(50);

        IsLoaded = true;
    }
